<SIRS>
	<SIR Name="execArbitraryCode">
		<Parameters>
			<Parameter Type="IR_Head">Host</Parameter>
			<Parameter Type="IR_Head">_Perm</Parameter>
		</Parameters>
		<Body>
			<Rule Name="execCode">
				<Parameters>
					<Parameter Type="Entity">Host</Parameter>
					<Parameter Type="Entity">_Perm</Parameter>
				</Parameters>
			</Rule>
			<Rule Name="vulExists">
				<Parameters>
					<Parameter Type="Entity">Host</Parameter>
					<Parameter Type="Entity">_</Parameter>
					<Parameter Type="Entity">Software</Parameter>
					<Parameter Type="Entity">_</Parameter>
					<Parameter Type="Entity">execArbitCode</Parameter>
				</Parameters>
			</Rule>
		</Body>
		<Description>Executing an arbitrary code</Description>
		<Technique>Exploitation for Client Execution</Technique>
	</SIR>
