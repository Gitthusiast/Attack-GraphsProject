	<SIR Name="notSame">
		<Parameters>
			<Parameter Type="IR_Head">H1</Parameter>
			<Parameter Type="IR_Head">H2</Parameter>
		</Parameters>
		<Body>
			<Rule Name="constraintLib:when">
				<Parameters>
					<Parameter Type="Entity">ground</Parameter>
					<Parameter Type="Entity">H1</Parameter>
					<Parameter Type="Entity">ground</Parameter>
					<Parameter Type="Entity">H2</Parameter>
					<Parameter Type="Entity">H1 \== H2</Parameter>
				</Parameters>
			</Rule>
		</Body>
		<Description>Not the same check</Description>
		<Technique>Building Block</Technique>
	</SIR>
	<SIR Name="notNetDev">
		<Parameters>
			<Parameter Type="IR_Head">H</Parameter>
		</Parameters>
		<Body>
			<Rule Name="constraintLib:when">
				<Parameters>
					<Parameter Type="Entity">ground</Parameter>
					<Parameter Type="Entity">H</Parameter>
					<Parameter Type="Entity">\+ networkHardware</Parameter>
					<Parameter Type="Entity">H</Parameter>
				</Parameters>
			</Rule>
		</Body>
		<Description>Not network device</Description>
		<Technique>Building Block</Technique>
	</SIR>
